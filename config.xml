<?xml version='1.0' encoding='utf-8'?>
<widget id="com.iareict.CS" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Computer Science</name>
    <description>
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
       IARE ICT Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="hdpi" src="res/icon/android/res/drawable-hdpi/ic_launcher.png" />
        <icon density="mdpi" src="res/icon/android/res/drawable-mdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/icon/android/res/drawable-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/icon/android/res/drawable-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="res/icon/android/res/drawable-xxxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
</widget>
